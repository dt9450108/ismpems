---
layout: app/app-layout
pagename: dashboard
---
<section class="section">
	<div class="row">
		<div class="col col-12 col-sm-12 col-md-6 col-xl-5 stats-col">
			{{> app/dashboard/items/items}}
		</div>
		<div class="col col-12 col-sm-12 col-md-6 col-xl-7">
            {{> app/dashboard/stats/stats}}
			<div class="card stats" data-exclude="xs">
				<div class="card-block">
					<div class="title-block">
						<h4 class="title">
							實驗室室內位置
						</h4>
					</div>
					<div id="dashboard-lab-layout" class="row row-sm stats-container">
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

